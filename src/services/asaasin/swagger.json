{
  "swagger": "2.0",
  "info": {
    "description": "Asaasin API documentation",
    "version": "1.0.0",
    "title": "Asaasin APIs",
    "contact": {
      "email": "navaneeth@lightwing.io"
    }
  },
  "host": "api.lightwing.io",
  "basePath": "/orgs",
  "tags": [
    {
      "name": "services",
      "description": "Auto-stopping APIs"
    }
  ],
  "schemes": ["https"],
  "paths": {
    "/github/all_users": {
      "get": {
        "tags": ["allGithubUsers"],
        "summary": "Gets allGithubUsers",
        "description": "allGithubUsers",
        "operationId": "allGithubUsers",
        "produces": ["application/json"],
        "responses": {
          "200": {
            "description": "successful operation",
            "schema": {
              "$ref": "#/definitions/AllGithubUsersResponse"
            }
          },
          "400": {
            "description": "Invalid status value"
          }
        }
      }
    },
    "/pagerduty/savings": {
      "get": {
        "tags": ["pagerdutySavings"],
        "summary": "Get pagerduty savings",
        "description": "pagerdutySavings",
        "operationId": "pagerdutySavings",
        "produces": ["application/json"],
        "responses": {
          "200": {
            "description": "successful operation",
            "schema": {
              "$ref": "#/definitions/PagerDutySavingsResponse"
            }
          },
          "400": {
            "description": "Invalid status value"
          }
        }
      }
    }
  },
  "definitions": {
    "GithubMember": {
      "type": "object",
      "properties": {
        "id": {
          "type": "integer",
          "format": "int64"
        },
        "name": {
          "type": "string"
        },
        "login": {
          "type": "string"
        },
        "avatar_url": {
          "type": "string"
        }
      }
    },
    "AllGithubUsersResponse": {
      "type": "array",
      "items": {
        "type": "object",
        "$ref": "#/definitions/GithubMember"
      }
    },
    "PagerDutySavings": {
      "type": "object",
      "properties": {
        "current_spend": {
          "type": "number"
        },
        "potential_spend": {
          "type": "number"
        },
        "potential_savings": {
          "type": "number"
        },
        "savings_percent": {
          "type": "number"
        }
      }
    },
    "PagerDutySavingsResponse": {
      "type": "object",
      "properties": {
        "total_users": {
          "type": "integer"
        },
        "active_users": {
          "type": "integer"
        },
        "monthly_savings": {
          "type": "object",
          "$ref": "#/definitions/PagerDutySavings"
        },
        "yearly_savings": {
          "type": "object",
          "$ref": "#/definitions/PagerDutySavings"
        },
        "optimization": {
          "type": "array",
          "items": {
            "type": "string"
          }
        }
      }
    }
  },
  "externalDocs": {
    "description": "Find out more about Swagger",
    "url": "http://swagger.io"
  }
}
