{
  "validate": true,
  "enableSchemaRequest": true,
  "hover": true,
  "completion": true,
  "schemas": [
    {
      "uri": "http://myserver/foo-schema.json",
      "fileMatch": ["*"],
      "schema": {
        "$schema": "http://json-schema.org/draft-07/schema",
        "$id": "http://example.com/example.json",
        "type": "object",
        "title": "The root schema",
        "description": "The root schema comprises the entire JSON document.",
        "default": {},
        "examples": [
          {
            "name": "string",
            "scope": "project",
            "project": "projectId",
            "identifier": "string",
            "value": "string",
            "kmsId": "string",
            "description": "string"
          }
        ],
        "required": ["name", "scope", "identifier", "kmsId"],
        "additionalProperties": true,
        "properties": {
          "name": {
            "$id": "#/properties/name",
            "type": "string",
            "examples": ["string"]
          },
          "scope": {
            "$id": "#/properties/scope",
            "enum": ["project", "organization", "account"],
            "examples": ["project"]
          },
          "project": {
            "$id": "#/properties/project",
            "type": "string",
            "examples": ["projectId"]
          },
          "identifier": {
            "$id": "#/properties/identifier",
            "type": "string",
            "examples": ["string"]
          },
          "value": {
            "$id": "#/properties/value",
            "type": "string",
            "examples": ["string"]
          },
          "kmsId": {
            "$id": "#/properties/kmsId",
            "type": "string",
            "examples": ["string"]
          },
          "description": {
            "$id": "#/properties/description",
            "type": "string",
            "examples": ["string"]
          }
        }
      }
    }
  ]
}
