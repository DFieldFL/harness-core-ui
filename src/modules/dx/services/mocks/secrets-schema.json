{
  "validate": true,
  "enableSchemaRequest": true,
  "hover": true,
  "completion": true,
  "schemas": [
    {
      "uri": "http://myserver/foo-schema.json",
      "fileMatch": ["*"],
      "schema": {
        "$schema": "http://json-schema.org/draft-07/schema",
        "$id": "http://example.com/example.json",
        "type": "object",
        "title": "The root schema",
        "description": "The root schema comprises the entire JSON document.",
        "default": {},
        "examples": [
          {
            "identifier": "string",
            "name": "string",
            "projectIdentifier": "projectId",
            "secretManager": "string",
            "description": "string"
          }
        ],
        "required": ["name", "identifier", "secretManager", "accountIdentifier"],
        "additionalProperties": false,
        "properties": {
          "name": {
            "$id": "#/properties/name",
            "type": "string",
            "examples": ["string"]
          },
          "projectIdentifier": {
            "$id": "#/properties/projectIdentifier",
            "type": "string",
            "examples": ["projectIdentifier"]
          },
          "accountIdentifier": {
            "$id": "#/properties/accountIdentifier",
            "type": "string",
            "examples": ["accountIdentifier"]
          },
          "identifier": {
            "$id": "#/properties/identifier",
            "type": "string",
            "examples": ["string"]
          },
          "secretManager": {
            "$id": "#/properties/kmsId",
            "type": "string",
            "examples": ["string"]
          },
          "description": {
            "$id": "#/properties/description",
            "type": "string",
            "examples": ["string"]
          }
        }
      }
    }
  ]
}
